  var container = document.querySelector('.hamburger-menu_1716879841568_210 .nav-container');\n    container.style.transform = 'translate(' + (data.scrollLeft - (offset.left/data.scale)) + 'px, ' + (data.scrollTop - (offset.top/data.scale)) + 'px)';\n  }\n  document.querySelector('.hamburger-menu_1716879841568_210 .hamburger-btn').addEventListener('click', function() { toggle(); });\n  document.addEventListener('resize', scrollOrResize);\n  document.addEventListener('scroll', scrollOrResize);\n  scrollOrResize()\n  setTimeout(() => scrollOrResize(), 0)\n})();\n</script>\n"},{"id":"silex-id-1716817314343-0","tagName":"SECTION","type":"section-element","alt":null,"title":null,"isSectionContent":false,"visibility":{"desktop":true,"mobile":true},"style":{"mobile":{},"desktop":{"position":"static"}},"data":{"component":null},"children":["silex-id-1716817314344-1"],"pageNames":["page-index"],"classList":[],"attr":{},"link":null,"enableEdit":false,"enableDrag":true,"enableDrop":true,"enableResize":{"top":false,"bottom":false,"left":false,"right":false},"selected":false,"useMinHeight":true,"innerHtml":""},{"id":"silex-id-1716817314344-1","tagName":"DIV","type":"container-element","alt":null,"title":null,"isSectionContent":true,"visibility":{"desktop":true,"mobile":true},"style":{"desktop":{"height":"235px","background-color":"rgb(255, 255, 255)","position":"relative","margin-left":"auto","margin-right":"auto"},"mobile":{}},"data":{"component":null},"children":[],"pageNames":[],"classList":["silex-element-content","website-width","prevent-draggable"],"attr":{},"link":null,"enableEdit":false,"enableDrag":false,"enableDrop":true,"enableResize":{"top":true,"bottom":true,"left":true,"right":true},"selected":false,"useMinHeight":true,"innerHtml":""},{"id":"silex-id-1716817811457-0","tagName":"DIV","type":"text-element","alt":null,"title":null,"isSectionContent":false,"visibility":{"desktop":true,"mobile":true},"style":{"mobile":{},"desktop":{"width":"898px","height":"469px","posi